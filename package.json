  },
  "keywords": [
    "language-detection",
    "file-language",
    "file-extension",
    "developer-tools",
    "static-analysis",
    "repository-analysis",
    "cli",
    "tooling"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alcanivorax/file-lang"
  },
  "author": "alcanivorax",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
