{
  "name": "file-lang",
  "version": "0.1.0",
  "description": "Deterministically detect a programming language from a file path, name, or extension.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
